@model PersonalJournal.Models.Models.Quote

@{
    ViewData["Title"] = "Home Page";
}

<div class="jumbotron text-center">
    <h1>Programmer Quotes To Live By</h1>
</div>

<div class="row">
    @foreach (var quote in Model.Quotes)
    {
        <div class="col-md-4 quote-container">
            <h4>
                @quote
            </h4>
        </div>
    }
</div>
